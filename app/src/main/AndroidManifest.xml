<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.tanjo.versioncodeviewer"
    android:versionCode="1"
    android:versionName="0.0.1" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="17" />

    <application
        android:icon="@drawable/ic_launcher_azazel"
        android:theme="@android:style/Theme.NoTitleBar"
        android:label="@string/app_name">
        <activity
            android:name="in.tanjo.versioncodeviewer.VersionCodeViewerMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="in.tanjo.versioncodeviewer.VersionCodeShowViewerActivity"
            android:icon="@drawable/ic_launcher_azazel">
        </activity>
        </application>

</manifest>